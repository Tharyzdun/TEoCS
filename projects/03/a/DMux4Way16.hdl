/**
 * 4-way 16-bit wise demultiplexor.
 * {a,b,c,d} = {in,0,0,0} if sel==00
 *             {0,in,0,0} if sel==01
 *             {0,0,in,0} if sel==10
 *             {0,0,0,in} if sel==11
 */

CHIP DMux4Way16 {
    IN in[16], sel[2];
    OUT a[16], b[16], c[16], d[16];

    PARTS:
	DMux16(in=in, sel=sel[1], a=first0, b=first1);
	DMux16(in=first0, sel=sel[0], a=a, b=b);
	DMux16(in=first1, sel=sel[0], a=c, b=d);
}